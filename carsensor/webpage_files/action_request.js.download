/****************************************
* reqVosVarPC
* 特定変数を出力
*****************************************/
function reqVosVarPC(en,ev,pn,pv) {
	if(typeof(s_gi)!="function"){
		var log="";
		if(en&&ev)log+=en+"="+ev;
		if(pn&&pv)log+=" "+pn+"="+pv;
		console.log("reqVosVarPC:"+log);return;
	}

	var ltv="";
	/* 初期化 */
	s=s_gi(s_account);

	if(en&&ev){
		ltv+=en;
		s[en]=ev;
	}
	if(pn&&pv){
		ltv+=(ltv)?','+pn:pn;
		s[pn]=pv;
	}

	if(ltv){
		s.linkTrackVars=ltv;
		s.tl(this,'o',ltv);
	}
}

